{
    "name": "mvdnbrk/dhlparcel-php-api",
    "description": "DHL Parcel API client for PHP",
    "keywords": [
        "dhl",
        "parcel",
        "api"
    ],
    "homepage": "https://github.com/mvdnbrk/dhlparcel-php-api",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "homepage": "https://github.com/mvdnbrk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "lcobucci/jwt": "^3.2",
        "ramsey/uuid": "^3.8",
        "tightenco/collect": "^6.0"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "nunomaduro/collision": "^3.0",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\DhlParcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\DhlParcel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Mvdnbrk\\DhlParcel\\DhlParcelServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
